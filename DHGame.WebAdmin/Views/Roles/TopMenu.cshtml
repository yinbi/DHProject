@model List<DHGame.Entity.DHWebSiteDB.RolesPermission>
@{
    Layout = null;
}
<ul>
    @for (int i = 0; i < Model.Count; i++)
    {
        if (i == 0)
        {
            <li class="selected"><a href="/Roles/LeftMenu/@Model[i].Modules.Id"><span>@Model[i].Modules.Title</span></a></li>
        }
        else
        {
            <li ><a href="/Roles/LeftMenu/@Model[i].Modules.Id"><span>@Model[i].Modules.Title</span></a></li>
        }
    }
</ul>
